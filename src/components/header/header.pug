.header
  .header__logo
    include ../logo/logo
    +logo('colorWithText')
  .header__wrapper
    include ../nav/nav
    .header__nav
      - let navList = ['О нас', 'Услуги', 'Вакансии', 'Новости', 'Соглашения']
      +navigation([{name: navList[0]}, {name: navList[1]}, {name: navList[2]}, {name: navList[3]}, {name: navList[4]}])
    .header__btns
      .header__buttonLogin
        include ../button/button
        +button({isWhite: true, btnTitle: 'войти'})
      .header__buttonRegister
        include ../button/button
        +button({default: true, arrow: true, btnTitle: 'зарегистрироваться'})
