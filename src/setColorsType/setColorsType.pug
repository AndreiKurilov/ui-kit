.setColorsType
  //- .setColorsType__blockName Colors & Type
  //- .setColorsType__commonLogoCircle
  //-   include ../headersFooters/commonBlocks/commonLogoCircle/commonLogoCircle
  
  -
    let colors = [colorsDarkShade100, colorsDarkShade75]
    
    let titleLetters = ['Dark Shade 100%', 'Dark Shade 75%', 'Dark Shade 50%', 'Dark Shade 25%', 'Dark Shade 5%', 'Purple', 'Green'];

    let titleCode = ['#1F2041', '#BC9CFF', '#6FCF97']
  
  .setColorsType__colors
    include ./colors/colors
    +color({color: colors[0], titleLetters: titleLetters[0], titleCode: titleCode[0]})
    +color({color: colors[1], titleLetters: titleLetters[1], titleCode: titleCode[0]})
    

  script.
    let colorsDarkShade100 = document.querySelector('.colors__darkShade100');
    let colorsDarkShade75 = document.querySelector('.colors__darkShade75');

    

